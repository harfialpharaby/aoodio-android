{
  "expo": {
    "name": "Aoodio",
    "slug": "aoodio",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "harfialfaraby.com",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "harfialfaraby.com",
      "versionCode": 1,
      "icon": "./assets/icon.png"
    },
    "description": "First react native challenge on Hacktiv8 with 3rd party API",
    "githubUrl": "https://github.com/harfialpharaby/react-native-challenge"
  }
}
